{
    "coc.preferences.formatOnSaveFiletypes": ["python", "json"],
    "coc.preferences.formatOnType": true,
    "coc.preferences.jumpCommand": "vsplit",
    "coc.preferences.useQuickfixForLocations": true,
    "coc.source.emoji.filetypes": ["gitcommit", "markdown"],
    "emmet.includeLanguages": { "htmldjango": "html" },
    "explorer.icon.enableNerdfont": true,
    "json.enable": true,
    "json.format.enable": true,
    "pydocstring.formatter": "google",
    "python.analysis.autoImportCompletions": true,
    "python.formatting.provider": "black",
    "python.jediEnabled": false,
    "python.linting.enabled": true,
    "python.linting.mypyEnabled": false,
    "python.linting.banditEnabled": true,
    "python.linting.pylamaEnabled": true,
    "python.venvFolders": ["envs", ".pyenv", "/home/onjin/.pyevn/versions/"],
    "yaml.trace.server": "verbose",
    "yaml.schemaStore.enable": true,
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        },
    }
}
